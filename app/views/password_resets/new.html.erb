<% provide(:title, "Forget password") %>
<h1>Forget password</h1>

<div class="row">
  <div class="co;-md-6 col-md-offset-3">
    <%= form_for(:password_reset, url: password_resets_path) do |f| %> <!--モデルがないので@password_reset出来ないので、リソース名とURLを明記してあげる-->
    <%= f.label :email %>
    <%= f.email_field :email, class: "form-control" %>
    <%= f.submit "submit", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>